
import numpy as np

from sympy import symbols

from sympy.physics import mechanics as mech

from sympy import Dummy, lambdify
from scipy.integrate import odeint

'''
Variable Defintions:

q = Generalized Coordinates
qD = Generalized Velocities
N = N-dimensions
m = mass
l = length
g = graivty
t = time
O = Reference Frame

'''

def integrate(N):

    q = mech.dynamicsymbols('q:{0}'.format(N))                   #Creates List of Generalized Coordinates w/ N-indices
    qD = mech.dynamicsymbols('qD:{0}'.format(N))                 #Creates List of Generalized Velocities w/ N-indices
    m = symbols('m:{0}'.format(N))                               #Creates List of Masses w/ N-indices
    l = symbols('l:{0}'.format(N))                               #Creates List of Lengths w/ N-indices
    g, t = symbols('g,t')                                        #Creates Gravity and Time Symbols


    O = mech.ReferenceFrame('O')                                 #Creates a Reference Frame
    P = mech.Point('P')                                          #Creates a Point (location)                 
    P.set_vel(O, 0)                                              #Places point P in Reference Frame w/ Zero velocity

    return(null)
